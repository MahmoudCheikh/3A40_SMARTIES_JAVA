<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.smarties.test.LoginController">
    <center>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="273.0">
            <children>
                <TextField fx:id="txtemail" layoutX="62.0" layoutY="52.0" />
                <TextField fx:id="txtpass" layoutX="62.0" layoutY="103.0" />
                <Button fx:id="btnLogin" layoutX="62.0" layoutY="146.0" mnemonicParsing="false" onAction="#login" text="Login" />
                <Button layoutX="139.0" layoutY="146.0" mnemonicParsing="false" onAction="#resetpass" text="Reset pass" />
                <Label layoutX="122.0" layoutY="27.0" text="Email" />
                <Label layoutX="101.0" layoutY="83.0" text="Mot de passe" />
                <Text fx:id="txterror" fill="RED" layoutX="73.0" layoutY="196.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="128.0" />
            </children>
        </AnchorPane>
    </center>
</BorderPane>
