<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.smarties.test.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="txtemail" layoutX="46.0" layoutY="53.0" />
            <TextField fx:id="txtpass" layoutX="46.0" layoutY="104.0" />
            <Button fx:id="btnLogin" layoutX="46.0" layoutY="147.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button layoutX="123.0" layoutY="147.0" mnemonicParsing="false" text="Reset pass" />
            <Label layoutX="106.0" layoutY="28.0" text="Email" />
            <Label layoutX="85.0" layoutY="84.0" text="Mot de passe" />
      </children>
    </AnchorPane>
  </children>
</VBox>
