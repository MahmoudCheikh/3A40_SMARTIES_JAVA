<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.smarties.test.GuiUsersController">
   <children>
      <Label layoutX="20.0" layoutY="14.0" prefHeight="36.0" prefWidth="206.0" text="Gestion User" textFill="#ff8c00">
         <font>
            <Font name="Cambria Math" size="26.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <ImageView fitHeight="108.0" fitWidth="132.0" layoutX="880.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons-logos-emojis-user-icon-png-transparent-11563566676e32kbvynug.png" />
         </image>
         <effect>
            <BoxBlur />
         </effect>
      </ImageView>
      <ListView fx:id="tableuserslist" layoutX="16.0" layoutY="60.0" onMouseClicked="#getDataUsers" prefHeight="419.0" prefWidth="689.0" style="-fx-background-color: #ff7f50;">
         <effect>
            <DropShadow />
         </effect></ListView>
      <TextField fx:id="reuser" layoutX="40.0" layoutY="486.0" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: #ff7f50;" />
      <Button fx:id="search" layoutX="164.0" layoutY="484.0" mnemonicParsing="false" onAction="#searchuser" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: #ff7f50;" text="Rechercher Id User" textFill="WHITE">
         <font>
            <Font name="Barlow Condensed Light" size="13.0" />
         </font></Button>
      <Button fx:id="actu" layoutX="287.0" layoutY="486.0" mnemonicParsing="false" onAction="#actuser" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: #ff7f50;" text="Actualisation" textFill="WHITE" />
      <ImageView fitHeight="17.0" fitWidth="23.0" layoutX="293.0" layoutY="490.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/png-transparent-computer-icons-synchronization-encapsulated-postscript-icon-design-update-button-angle-text-logo.png" />
         </image>
      </ImageView>
      <Button layoutX="410.0" layoutY="486.0" mnemonicParsing="false" onAction="#ban" text="Ban" />
      <Button layoutX="446.0" layoutY="486.0" mnemonicParsing="false" onAction="#unban" text="Unban" />
      <Button layoutX="497.0" layoutY="486.0" mnemonicParsing="false" onAction="#ban" text="Confirmer" />
   </children>
</AnchorPane>
